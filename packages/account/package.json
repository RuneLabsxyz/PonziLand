{
	"name": "@runelabsxyz/ponziland-account",
	"version": "0.6.1",
	"repository": {
		"url": "https://github.com/RuneLabsxyz/PonziLand",
		"directory": "packages/account"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"pack": "npm run prepack && bun pm pack --ignore-scripts",
		"test": "npm run test:unit -- --run",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write ."
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"dependencies": {
		"@argent/x-sessions": "catalog:",
		"@cartridge/controller": "catalog:",
		"@dojoengine/sdk": "catalog:dojo",
		"@dojoengine/torii-client": "catalog:dojo",
		"@lucide/svelte": "catalog:",
		"@ponziland/ui": "workspace:*",
		"@solana/web3.js": "catalog:",
		"@starknet-io/get-starknet-core": "catalog:",
		"@starknet-io/types-js": "catalog:",
		"starknet": "catalog:",
		"zustand": "catalog:dojo",
		"bignumber.js": "catalog:"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"eslint-plugin-unicorn": "^61.0.2",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@types/node": "^22",
		"@vitest/browser": "^3.2.4",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"playwright": "^1.55.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.13",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"keywords": [
		"svelte"
	]
}
