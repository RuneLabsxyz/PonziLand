{
  "name": "@ponziland/account",
  "version": "0.3.0",
  "description": "Account management library for Starknet with wallet modal and connection state",
  "keywords": [
    "starknet",
    "wallet",
    "account",
    "svelte",
    "sveltekit"
  ],
  "homepage": "https://ponzi.land",
  "license": "MIT",
  "author": {
    "name": "PonziLand Team"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RuneLabsxyz/PonziLand/tree/main/pkgs/account"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "starknet": "^8.0.0",
    "@argent/x-sessions": "^8.0.0",
    "@cartridge/controller": ">=0.9.2",
    "@dojoengine/sdk": "^1.7.0",
    "@starknet-io/get-starknet-core": "^4.0.0",
    "lucide-svelte": ">=0.471.0",
    "bignumber.js": "^9.1.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "publint": "^0.3.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.4",
    "@runelabsxyz/socialink-sdk": "0.1.1",
    "starknet": "^8.0.0",
    "@argent/x-sessions": "^8.0.0",
    "@cartridge/controller": ">=0.9.2",
    "@dojoengine/sdk": "^1.7.0",
    "@starknet-io/get-starknet-core": "^4.0.0",
    "lucide-svelte": ">=0.471.0",
    "bignumber.js": "^9.1.2"
  },
  "dependencies": {
    "@ponziland/ui": "0.1.0",
    "@solana/web3.js": "^1.98.4"
  }
}
